{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "NodeNext",
	  "moduleResolution": "nodenext",
	  "jsx": "react-jsx",
	  "strict": true,
	  "allowSyntheticDefaultImports": true,
	  "baseUrl": ".",
	  "paths": {
		"@/*": ["./*"],
		"@/components/*": ["./src/components/*"],
		"@/lib/*": ["./src/lib/*"],
		"@/hooks/*": ["./src/hooks/*"],
		"@/utils/*": ["./src/utils/*"]
	  }
	}
}
