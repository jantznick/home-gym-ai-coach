		"@types/react-dom": "^19.0.4",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"date-fns": "^3.6.0",
		"lucide-react": "^0.373.0",
		"react": "^18.2.0",
		"react-day-picker": "^8.10.1",
