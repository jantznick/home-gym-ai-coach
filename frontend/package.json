{
	"name": "fitness-frontend",
	"version": "1.0.0",
	"description": "React frontend for My Fitness App",
	"private": true,
	"scripts": {
		"dev": "npm run watch-css & webpack serve --mode development --open",
		"build": "webpack --mode production",
