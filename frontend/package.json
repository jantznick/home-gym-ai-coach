{
	"name": "fitness-frontend",
	"version": "1.0.0",
	"description": "React frontend for My Fitness App",
	"private": true,
	"scripts": {
		"dev": "npm run watch-css & webpack serve --mode development --open",
		"build": "webpack --mode production",
		"start": "serve -s dist",
		"build-css": "tailwindcss -i ./src/styles/index.css -o ./public/css/style.css",
		"watch-css": "tailwindcss -i ./src/styles/index.css -o ./public/css/style.css --watch"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^7.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.26.10",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"autoprefixer": "^10.4.21",
		"babel-loader": "^9.2.1",
		"css-loader": "^7.1.2",
		"html-webpack-plugin": "^5.6.3",
		"postcss": "^8.5.3",
		"sass": "^1.85.1",
		"sass-loader": "^16.0.5",
		"serve": "^14.2.0",
		"style-loader": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"ts-loader": "^9.5.0",
		"typescript": "^5.0.2",
		"webpack": "^5.98.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.13.0"
	}
}
